<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tictac.aaditya.com.tictactoenew">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Replay" android:label="Game Over" />
        <activity android:name=".SinglePlayer" android:label="Single Player"/>
        <activity android:name=".TwoPlayer" android:label="Two Players"></activity>
    </application>

</manifest>